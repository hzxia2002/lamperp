<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.justonetech.system.domain">
	<class
		name="ConfigTable"
		table="CONFIG_TABLE"
	>

		<meta attribute="sync-DAO">false</meta>
		<meta attribute="sync-jsp">true</meta>
		<meta attribute="sync-boolean">get</meta>
		<meta attribute="treeable">false</meta>
		<meta attribute="projectable">false</meta>
        <meta attribute="tableName">表日志配置</meta>
        <meta attribute="tableComment">表日志配置</meta>
	    <meta attribute="sync-templatePatterns">list\w*</meta>
        <meta attribute="sub-system"></meta>

        <!--<cache usage="read-write"/>-->

		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="com.justonetech.core.orm.hibernate.LongIdGenerator"/>
		</id>

		<property
			name="tableName"
			column="TABLE_NAME"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">表名</meta>
            <meta attribute="ColumnComment">表名</meta>
        </property>

        <property
                name="className"
                column="CLASS_NAME"
                type="string"
                not-null="false"
                length="100"
                >
            <meta attribute="ColumnName">类名</meta>
            <meta attribute="ColumnComment">类名</meta>
        </property>

		<property
			name="isLog"
			column="IS_LOG"
			type="java.lang.Boolean"
			not-null="false"
			length="1"
        >
            <meta attribute="ColumnName">是否记录日志</meta>
            <meta attribute="ColumnComment">是否记录日志</meta>
        </property>

		<property
			name="extendXml"
			column="EXTEND_XML"
			type="string"
			not-null="false"
			length="1000"
        >
            <meta attribute="ColumnName">备用</meta>
            <meta attribute="ColumnComment">备用</meta>
        </property>

		<property
			name="createTime"
			column="CREATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">创建时间</meta>
            <meta attribute="ColumnComment">创建时间</meta>
        </property>

		<property
			name="updateTime"
			column="UPDATE_TIME"
			type="timestamp"
			not-null="false"
			length="7"
        >
            <meta attribute="ColumnName">更新时间</meta>
            <meta attribute="ColumnComment">更新时间</meta>
        </property>

		<property
			name="updateUser"
			column="UPDATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">更新人</meta>
            <meta attribute="ColumnComment">更新人(记录帐号）</meta>
        </property>

		<property
			name="createUser"
			column="CREATE_USER"
			type="string"
			not-null="false"
			length="100"
        >
            <meta attribute="ColumnName">创建人</meta>
            <meta attribute="ColumnComment">创建人(记录帐号）</meta>
        </property>

	
<!--

		<set name="sysOperationTableLogs" inverse="true">
			<key column="TABLE_ID"/>
			<one-to-many class="SysOperationTableLog"/>
		</set>

-->


	</class>	
    <query name="configtable_all_data_query">
        <![CDATA[FROM ConfigTable]]>
    </query>
</hibernate-mapping>